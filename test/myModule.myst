trace = fn m y {
  unsafePerformIO do IO {
    console.log m y;
    IO.return y;
  }
};

fact = fn x {
  _IF_ (x <= 0)
    1
    (x * fact (x - 1))
};

test1 = { Hello: "There!", Whats: 9+9 };

test2 = ["Hello", "Again", 9+3+8];

main = do IO {
  console.log test1;
  console.log test2;
  console.log "Hello World!";
  console.log "This is a pretty cool place!";
  console.log (fact 6);
};
